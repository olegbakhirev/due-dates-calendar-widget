{
  "name": "due-dates-calendar",
  "version": "1.0.0",
  "private": true,
  "config": {
    "components": "./src",
    "dist": "./dist"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "create-component": "yo @jetbrains/ring-ui:react --path $npm_package_config_components",
    "build": "webpack -p --devtool sourcemap",
    "test": "karma start",
    "lint": "eslint . && npm run stylelint",
    "stylelint": "stylelint --ignore-path .gitignore **/*.css",
    "ci-test": "npm run lint && xvfb-maybe karma start --teamcity",
    "start": "webpack-dev-server -d --history-api-fallback --inline --hot --colors --port 9010"
  },
  "babel": {
    "presets": "@jetbrains/jetbrains"
  },
  "dependencies": {
    "@jetbrains/babel-preset-jetbrains": "1.1.0",
    "@jetbrains/hub-widget-ui": "^1.0.28",
    "@jetbrains/icons": "1.20.1",
    "@jetbrains/logos": "1.0.5",
    "@jetbrains/ring-ui": "1.0.0-beta.191",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "classnames": "^2.2.6",
    "css-loader": "0.28.9",
    "file-loader": "1.1.6",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "2.30.1",
    "hub-dashboard-addons": "0.1.2",
    "moment": "^2.23.0",
    "node-sass": "^4.9.4",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.1.0",
    "postcss-modules-values-replace": "3.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-big-calendar": "^0.21.0",
    "react-dom": "^16.6.3",
    "redbox-react": "1.5.0",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  },
  "devDependencies": {
    "@jetbrains/eslint-config": "2.0.0",
    "@jetbrains/generator-ring-ui": "0.4.59",
    "@jetbrains/stylelint-config": "1.3.2",
    "babel-eslint": "8.2.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-dom": "1.7.0",
    "chai-enzyme": "1.0.0-beta.0",
    "electron": "1.8.2",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.18.2",
    "karma": "2.0.0",
    "karma-chai-plugins": "0.9.0",
    "karma-electron-launcher": "0.2.0",
    "karma-mocha": "1.3.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.1.0",
    "karma-webpack": "2.0.9",
    "mocha": "5.0.1",
    "react-test-renderer": "16.2.0",
    "sinon": "4.3.0",
    "sinon-chai": "2.14.0",
    "stylelint": "^9.10.1",
    "xvfb-maybe": "0.2.1",
    "yo": "2.0.1"
  },
  "engines": {
    "node": ">=6.0.0"
  }
